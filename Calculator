class Calculator:
    
    def __init__(self, a, b):
        self.a = a
        self.b = b
        
    def add(self):
        return self.a + self.b
    
    def substruct(self):
        return self.a - self.b
    
    def multiplly(self):
        return self.a * self.b
    
    def divide(self):
        return self.a / self.b